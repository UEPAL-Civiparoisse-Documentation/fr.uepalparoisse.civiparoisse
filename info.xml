<?xml version="1.0"?>
<extension key="fr.uepalparoisse.civiparoisse" type="module">
  <file>civiparoisse</file>
  <name>Civiparoisse</name>
  <description>Civiparoisse : module de gestion op&#xE9;rationnelle de paroisse pour CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>UEPAL (Union des Eglises Protestantes d'Alsace et de Moselle)</author>
    <email>civiparoisse@uepal.fr</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/UEPAL-Civiparoisse-Documentation</url>
    <url desc="Documentation">https://uepal-civiparoisse-documentation.github.io/</url>
    <url desc="Support">To be provided</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-05-01</releaseDate>
  <version>1.51.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.51</ver>
  </compatibility>
  <requires>
    <ext>org.civicrm.search_kit</ext>
    <ext>org.civicrm.afform</ext>
  </requires>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Civiparoisse</namespace>
    <format>25.01.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\Civiparoisse\AutomaticUpgrader</upgrader>
</extension>
